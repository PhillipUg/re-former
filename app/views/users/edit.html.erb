<%= form_for @user do |f| %>
		<%= f.label :Username %><br>
		<%= f.text_field :username, placeholder: "Username" %><br>

		<%= f.label :Email %><br>
		<%= f.email_field :email, placeholder: "example@example.com" %><br>

		<%= f.label :Password %><br>
		<%= f.password_field :password %><br><br>

		<%= f.submit %>
<% end %>
